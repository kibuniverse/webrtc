#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/yankaizhi/private/webrtc/node_modules/.pnpm/registry.nlark.com/envinfo@7.8.1/node_modules/envinfo/dist/cli.js/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/registry.nlark.com/envinfo@7.8.1/node_modules/envinfo/dist/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/registry.nlark.com/envinfo@7.8.1/node_modules/envinfo/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/registry.nlark.com/envinfo@7.8.1/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/registry.nlark.com/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/node_modules:/Users/yankaizhi/private/webrtc/node_modules:/Users/yankaizhi/private/node_modules:/Users/yankaizhi/node_modules:/Users/node_modules:/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/node_modules/envinfo/dist/cli.js/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/node_modules/envinfo/dist/node_modules:/Users/yankaizhi/private/webrtc/node_modules/.pnpm/node_modules/envinfo/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../envinfo/dist/cli.js" "$@"
else
  exec node  "$basedir/../envinfo/dist/cli.js" "$@"
fi
